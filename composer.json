{
    "name": "dangtrungkien/ytb",
    "description": "Upload a video to YouTube with Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dang Kien",
            "email": "dangtrungkien96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4",
        "google/apiclient": "^2.2",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "phpunit/phpunit": "~5.0",
        "phpunit/phpcov": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DangKien\\Youtube\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DangKien\\Youtube\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DangKien\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "DangKien\\Youtube\\Facades\\Youtube"
            }
        }
    }
}
